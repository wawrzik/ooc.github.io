###  https://www.w3id.org/people/wawrzik/ooc#Truth
:Truth rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericalValue> ;
                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                  owl:onDatatype xsd:integer ;
                                                  owl:withRestrictions ( [ xsd:minInclusive 200
                                                                         ]
                                                                       )
                                                ]
                           ] ;
       rdfs:subClassOf :level_of_consciousness ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericalValue> ;
                         owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                             owl:onDatatype xsd:integer ;
                                             owl:withRestrictions ( [ xsd:minInclusive 200
                                                                    ]
                                                                  )
                                           ]
                       ] ;
       rdfs:label "Truth" ,
                  "integrity" .


